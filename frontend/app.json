{
  "expo": {
    "name": "UCVGreenMobility",
    "slug": "UCVGreenMobility",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./frontend/assets/images/icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./frontend/assets/images/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0B6A75"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./frontend/assets/images/icon.jpg",
        "backgroundColor": "#0B6A75"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./frontend/assets/images/icon.jpg"
    }
  }
}
